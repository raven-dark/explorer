{
  "network": "livenet",
  "port": 80,
  "services": [
    "dashd",
    "@dashevo/insight-api",
    "@dashevo/insight-ui",
    "web"
  ],
  "servicesConfig": {
    "dashd": {
      "spawn": {
        "datadir": "./data",
        "exec": "/ravendark/ravendarkd"
      }
    },
    "@dashevo/insight-ui": {
      "routePrefix": "",
      "apiPrefix": "api"
    },
    "@dashevo/insight-api": {
      "routePrefix": "api",
      "enableCache": true
    }
  }
}
